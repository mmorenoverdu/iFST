        <Param val="fb_dur = 3" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="fb_dur = 3;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="psychoJS.window.mouseVisible = false;&amp;#10;current_stim = Number.parseInt(stim);&amp;#10;if ((block_number &lt;= practice_blocks)) {&amp;#10;    if ((seq_acc === 1)) {&amp;#10;        fb_dur = 3;&amp;#10;        corr_vol = 1;&amp;#10;        incorr_vol = 0;&amp;#10;        stim_counters[current_stim] += 1;&amp;#10;    } else {&amp;#10;        if ((seq_acc === 0)) {&amp;#10;            fb_dur = 5;&amp;#10;            corr_vol = 0;&amp;#10;            incorr_vol = 1;&amp;#10;        }&amp;#10;    }&amp;#10;    if ((stim_counters[current_stim] &lt;= target_per_stim)) {&amp;#10;        continueRoutine = true;&amp;#10;    }&amp;#10;    if ((stim_counters[current_stim] === 100)) {&amp;#10;        continueRoutine = false;&amp;#10;        stim_counters[current_stim] = 100;&amp;#10;    }&amp;#10;    psychoJS.experiment.addData(&quot;crit_counter&quot;, stim_counters[current_stim]);&amp;#10;} else {&amp;#10;    continueRoutine = false;&amp;#10;}&amp;#10;if ((trial_type === &quot;imagery&quot;)) {&amp;#10;    color_text = &quot;purple&quot;;&amp;#10;    if ((seq_acc === 1)) {&amp;#10;        fb_message = correct_ima_msg;&amp;#10;    } else {&amp;#10;        if ((seq_acc === 0)) {&amp;#10;            fb_message = incorrect_ima_msg;&amp;#10;        }&amp;#10;    }&amp;#10;} else {&amp;#10;    if ((trial_type === &quot;execution&quot;)) {&amp;#10;        color_text = &quot;blue&quot;;&amp;#10;        if ((seq_acc === 1)) {&amp;#10;            fb_message = correct_exe_msg;&amp;#10;        } else {&amp;#10;            if ((seq_acc === 0)) {&amp;#10;                fb_message = incorrect_exe_msg;&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="win.mouseVisible = False&amp;#10;current_stim = int(stim)&amp;#10;if block_number &lt;= practice_blocks:&amp;#10;    if seq_acc == 1:&amp;#10;        fb_dur = 3&amp;#10;        corr_vol = 1&amp;#10;        incorr_vol = 0&amp;#10;        stim_counters[current_stim] += 1&amp;#10;    elif seq_acc == 0:&amp;#10;        fb_dur = 5&amp;#10;        corr_vol = 0&amp;#10;        incorr_vol = 1&amp;#10;        # practice block, show always if counter is not reached&amp;#10;    if stim_counters[current_stim] &lt;= target_per_stim:&amp;#10;        continueRoutine = True&amp;#10;    if stim_counters[current_stim] == 100:&amp;#10;        continueRoutine = False&amp;#10;        stim_counters[current_stim] = 100 # keep it&amp;#10;    thisExp.addData(&quot;crit_counter&quot;, stim_counters[current_stim])&amp;#10;else:&amp;#10;    continueRoutine = False&amp;#10;&amp;#10;if trial_type == &quot;imagery&quot;:&amp;#10;    color_text = &quot;purple&quot;&amp;#10;    if seq_acc == 1:&amp;#10;        fb_message = correct_ima_msg&amp;#10;    elif seq_acc == 0:&amp;#10;        fb_message = incorrect_ima_msg&amp;#10;elif trial_type == &quot;execution&quot;:&amp;#10;    color_text = &quot;blue&quot;&amp;#10;    if seq_acc == 1:&amp;#10;        fb_message = correct_exe_msg&amp;#10;    elif seq_acc == 0:&amp;#10;        fb_message = incorrect_exe_msg" valType="extendedCode" updates="constant" name="Begin Routine"/>
