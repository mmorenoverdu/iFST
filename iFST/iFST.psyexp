    <Routine name="seq_trial">
      <CodeComponent name="seq_trial_code" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="key_count = 0&amp;#10;prompt = &quot;&quot;&amp;#10;fill1 = &quot;&quot;&amp;#10;fill2 = &quot;&quot;&amp;#10;fill3 = &quot;&quot;&amp;#10;fill4 = &quot;&quot;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="key_count = 0;&amp;#10;prompt = &quot;&quot;;&amp;#10;fill1 = &quot;&quot;;&amp;#10;fill2 = &quot;&quot;;&amp;#10;fill3 = &quot;&quot;;&amp;#10;fill4 = &quot;&quot;;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="current_stim = Number.parseInt(stim);&amp;#10;if ((block_number &gt; practice_blocks)) {&amp;#10;    if ((sequence_type === sequence)) {&amp;#10;        continueRoutine = true;&amp;#10;    } else {&amp;#10;        continueRoutine = false;&amp;#10;    }&amp;#10;}&amp;#10;if ((block_number &lt;= practice_blocks)) {&amp;#10;    if ((stim_counters[current_stim] &gt;= target_per_stim)) {&amp;#10;        continueRoutine = false;&amp;#10;    }&amp;#10;}&amp;#10;[fill1, fill2, fill3, fill4] = [&quot;black&quot;, &quot;black&quot;, &quot;black&quot;, &quot;black&quot;];&amp;#10;seq_timer = new util.Clock();&amp;#10;seq_timer.reset();&amp;#10;previous_key_count = 0;&amp;#10;current_key_count = 0;&amp;#10;last_key = &quot;&quot;;&amp;#10;psychoJS.window.mouseVisible = false;&amp;#10;seq_resp.keys = [];&amp;#10;key_count = 0;&amp;#10;seq_resp.rt = [];&amp;#10;if ((trial_type === &quot;imagery&quot;)) {&amp;#10;    prompt = prompt_ima_msg;&amp;#10;    color_text = &quot;purple&quot;;&amp;#10;} else {&amp;#10;    if ((trial_type === &quot;execution&quot;)) {&amp;#10;        prompt = prompt_exe_msg;&amp;#10;        color_text = &quot;blue&quot;;&amp;#10;    }&amp;#10;}&amp;#10;console.log('BEGIN ROUTINE name=RUTINA_X:', {&amp;#10;  block_number: block_number,&amp;#10;  practice_blocks: practice_blocks,&amp;#10;  sequence_type: sequence_type,&amp;#10;  sequence: sequence,&amp;#10;  time: performance.now()&amp;#10;});" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# Trial settings depending on condition&amp;#10;current_stim = int(stim)&amp;#10;# in test blocks, skip routine based on counterbalancing&amp;#10;if block_number &gt; practice_blocks:&amp;#10;    if sequence_type == sequence:&amp;#10;        continueRoutine = True&amp;#10;    else:&amp;#10;        continueRoutine = False&amp;#10;# in practice blocks, show only if counter is not complete&amp;#10;if block_number &lt;= practice_blocks:&amp;#10;    if stim_counters[current_stim] &gt;= target_per_stim:&amp;#10;        continueRoutine = False&amp;#10;&amp;#10;# Initialize variables&amp;#10;fill1, fill2, fill3, fill4 = 'black', 'black', 'black', 'black'  # Default color&amp;#10;seq_timer = core.Clock() # create timer&amp;#10;seq_timer.reset() # start timer at 0&amp;#10;# Initialize variables to count keys and end routine&amp;#10;# based on number of key presses&amp;#10;previous_key_count = 0&amp;#10;current_key_count = 0&amp;#10;last_key = &quot;&quot;&amp;#10;win.mouseVisible = False&amp;#10;seq_resp.keys = []&amp;#10;key_count = 0&amp;#10;seq_resp.rt = []&amp;#10;# change color and prompt based on condition (old)&amp;#10;if trial_type == &quot;imagery&quot;:&amp;#10;    prompt = prompt_ima_msg&amp;#10;    color_text = &quot;purple&quot;&amp;#10;elif trial_type == &quot;execution&quot;:&amp;#10;    prompt = prompt_exe_msg&amp;#10;    color_text = &quot;blue&quot;&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Both" valType="str" updates="None" name="Code Type"/>
        <Param val="&amp;#10;# At the beginning, no key has been pressed&amp;#10;# We initialize the list of key presses&amp;#10;if seq_resp.keys is None:&amp;#10;    seq_resp.keys = []&amp;#10;&amp;#10;# set variable to count the number of keys&amp;#10;current_key_count = len(seq_resp.keys) # use the native variable from component&amp;#10;&amp;#10;# Check if a key has been pressed&amp;#10;if current_key_count &gt; previous_key_count:&amp;#10;    key_count += 1&amp;#10;    last_key = seq_resp.keys[-1]  # Get the latest key pressed&amp;#10;    seq_timer.reset()  # Reset the timer to 0 whenever a new key is pressed&amp;#10;    if feedback == True:&amp;#10;        if last_key == &quot;f&quot;:&amp;#10;            fill1, fill2, fill3, fill4 = 'white', 'black', 'black', 'black'&amp;#10;        elif last_key == &quot;g&quot;:&amp;#10;            fill1, fill2, fill3, fill4 = 'black', 'white', 'black', 'black'&amp;#10;        elif last_key == &quot;h&quot;:&amp;#10;            fill1, fill2, fill3, fill4 = 'black', 'black', 'white', 'black'&amp;#10;        elif last_key == &quot;j&quot;:&amp;#10;            fill1, fill2, fill3, fill4 = 'black', 'black', 'black', 'white'&amp;#10;    elif feedback == False:&amp;#10;        fill1 = fill2 = fill3 = fill4 = 'black'&amp;#10;else:&amp;#10;    key_count = key_count&amp;#10;&amp;#10;# Update the previous key count for the next frame&amp;#10;previous_key_count = current_key_count&amp;#10;&amp;#10;# Check timer and get back to black after a couple of ms&amp;#10;if seq_timer.getTime() &gt;= 0.1:&amp;#10;    fill1, fill2, fill3, fill4 = 'black', 'black', 'black', 'black'&amp;#10;&amp;#10;# End trial when reaching expected number of keys&amp;#10;if key_count == key_presses:&amp;#10;    continueRoutine = False&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="if ((block_number &gt; practice_blocks) &amp;&amp; (sequence_type !== sequence)) {&amp;#10;    continueRoutine = false;&amp;#10;}&amp;#10;&amp;#10;if ((seq_resp.keys === undefined)) {&amp;#10;    seq_resp.keys = [];&amp;#10;}&amp;#10;current_key_count = seq_resp.keys.length;&amp;#10;if ((current_key_count &gt; previous_key_count)) {&amp;#10;    key_count += 1;&amp;#10;    last_key = seq_resp.keys.slice((- 1))[0];&amp;#10;    seq_timer.reset();&amp;#10;    if ((feedback === true)) {&amp;#10;        if ((last_key === &quot;f&quot;)) {&amp;#10;            [fill1, fill2, fill3, fill4] = [&quot;white&quot;, &quot;black&quot;, &quot;black&quot;, &quot;black&quot;];&amp;#10;        } else {&amp;#10;            if ((last_key === &quot;g&quot;)) {&amp;#10;                [fill1, fill2, fill3, fill4] = [&quot;black&quot;, &quot;white&quot;, &quot;black&quot;, &quot;black&quot;];&amp;#10;            } else {&amp;#10;                if ((last_key === &quot;h&quot;)) {&amp;#10;                    [fill1, fill2, fill3, fill4] = [&quot;black&quot;, &quot;black&quot;, &quot;white&quot;, &quot;black&quot;];&amp;#10;                } else {&amp;#10;                    if ((last_key === &quot;j&quot;)) {&amp;#10;                        [fill1, fill2, fill3, fill4] = [&quot;black&quot;, &quot;black&quot;, &quot;black&quot;, &quot;white&quot;];&amp;#10;                    }&amp;#10;                }&amp;#10;            }&amp;#10;        }&amp;#10;    } else {&amp;#10;        if ((feedback === false)) {&amp;#10;            fill1 = fill2 = fill3 = fill4 = &quot;black&quot;;&amp;#10;        }&amp;#10;    }&amp;#10;} else {&amp;#10;    key_count = key_count;&amp;#10;}&amp;#10;previous_key_count = current_key_count;&amp;#10;if ((seq_timer.getTime() &gt;= 0.1)) {&amp;#10;    [fill1, fill2, fill3, fill4] = [&quot;black&quot;, &quot;black&quot;, &quot;black&quot;, &quot;black&quot;];&amp;#10;}&amp;#10;if ((key_count === key_presses)) {&amp;#10;    continueRoutine = false;&amp;#10;}&amp;#10;console.log('EACH FRAME name=RUTINA_X (start):', {&amp;#10;  block_number: block_number,&amp;#10;  practice_blocks: practice_blocks,&amp;#10;  sequence_type: sequence_type,&amp;#10;  sequence: sequence,&amp;#10;  key_count: typeof key_count !== 'undefined' ? key_count : null,&amp;#10;  key_presses: typeof key_presses !== 'undefined' ? key_presses : null,&amp;#10;  continueRoutine_now: typeof continueRoutine !== 'undefined' ? continueRoutine : 'undef',&amp;#10;  time: performance.now()&amp;#10;});" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="seq_typed = seq_resp.keys.join(&quot;&quot;);&amp;#10;seq_acc = ((seq_typed === sequence_code) ? 1 : 0);&amp;#10;if ((seq_resp.rt &amp;&amp; (seq_resp.rt.length &gt;= key_presses))) {&amp;#10;    last_press = (key_presses - 1);&amp;#10;    seq_time = (seq_resp.rt[last_press] - seq_resp.rt[0]);&amp;#10;    seq_time = util.round(seq_time, 2);&amp;#10;} else {&amp;#10;    seq_time = 999;&amp;#10;}&amp;#10;psychoJS.experiment.addData(&quot;seq_typed&quot;, seq_typed);&amp;#10;psychoJS.experiment.addData(&quot;seq_acc&quot;, seq_acc);&amp;#10;psychoJS.experiment.addData(&quot;seq_time&quot;, seq_time);&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="# create a string with the key presses&amp;#10;seq_typed = &quot;&quot;.join(seq_resp.keys)&amp;#10;# the output will be &quot;fghjjhgf&quot;&amp;#10;&amp;#10;# compare with variable hosting correct response&amp;#10;seq_acc = 1 if seq_typed == sequence_code else 0&amp;#10;&amp;#10;# calculate time to finish the sequence&amp;#10;# as the trial does not finish until a given N of key presses&amp;#10;# we can calculate the time as the last minus the first items&amp;#10;# regardless of the type of trial&amp;#10;# we use key_presses as this variable may change every trial depending&amp;#10;# on trial type (e.g. execution vs imagery)&amp;#10;if seq_resp.rt and len(seq_resp.rt) &gt;= key_presses:&amp;#10;    last_press = key_presses - 1  # Index of last expected press&amp;#10;    seq_time = seq_resp.rt[last_press] - seq_resp.rt[0]&amp;#10;    seq_time = round(seq_time, 2)&amp;#10;else:&amp;#10;    seq_time = 999  # Fallback in case of missing responses&amp;#10;# save to output&amp;#10;thisExp.addData(&quot;seq_typed&quot;, seq_typed)&amp;#10;thisExp.addData(&quot;seq_acc&quot;, seq_acc)&amp;#10;thisExp.addData(&quot;seq_time&quot;, seq_time)" valType="extendedCode" updates="constant" name="End Routine"/>
